{{ form_start(form) }}

    <div class="form-control">
        {{ form_label(form.name) }}
        <div class="form-error">
            {{ form_errors(form.name) }}
        </div>
        {{ form_widget(form.name) }}
        <small>{{ form_help(form.name) }}</small>
    </div>
    <br> <br>

    <button type="submit" >{{ button_label|default('Save') }}</button>

{{ form_end(form) }}

